{
  "extends": "airbnb/base",
  "rules": {
    "comma-dangle": 0,
    "max-len": [2, 120, 4],
    "quote-props": [2, "as-needed", { "keywords": true }],
    "object-curly-spacing": [2, "never"],
    "object-shorthand": 0,
    "prefer-const": 0,
    "no-param-reassign": 0,
    "new-cap": [2, {"capIsNewExceptions": ["express.Router"]}],
    "arrow-body-style": [2, "always"],
    "no-shadow": [1, {"allow": ["next", "err"]}],
    "no-use-before-define": 0
  }
}
